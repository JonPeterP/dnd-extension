{
    "manifest_version": 3,
    "name": "DnD Chrome Extension",
    "description": "Chrome Extension for DnD. Personal Use",
    "version": "1.0.0",
    "action":{
        "default_popup": "index.html",
        "default_title": "Dnd Roll"
    },
    "permissions": [
        "scripting", "activeTab", "storage"
    ],
    "host_permissions":["<all_urls>"]
}